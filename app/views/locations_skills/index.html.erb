<h1>Listing locations_skills</h1>

<table>
  <thead>
    <tr>
      <th>Location</th>
      <th>Skill</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @locations_skills.each do |locations_skill| %>
      <tr>
        <td><%= locations_skill.location %></td>
        <td><%= locations_skill.skill %></td>
        <td><%= locations_skill.user %></td>
        <td><%= link_to 'Show', locations_skill %></td>
        <td><%= link_to 'Edit', edit_locations_skill_path(locations_skill) %></td>
        <td><%= link_to 'Destroy', locations_skill, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Locations skill', new_locations_skill_path %>
