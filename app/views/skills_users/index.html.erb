<h1>Listing skills_users</h1>

<table>
  <thead>
    <tr>
      <th>Skill</th>
      <th>User</th>
      <th>Proficiency level</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @skills_users.each do |skills_user| %>
      <tr>
        <td><%= skills_user.skill %></td>
        <td><%= skills_user.user %></td>
        <td><%= skills_user.proficiency_level %></td>
        <td><%= link_to 'Show', skills_user %></td>
        <td><%= link_to 'Edit', edit_skills_user_path(skills_user) %></td>
        <td><%= link_to 'Destroy', skills_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Skills user', new_skills_user_path %>
